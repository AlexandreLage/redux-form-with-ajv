{
  "extends": "eslint-config-rackt",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-no-undef": 0,
    "no-console": "off",
    "no-unused-vars": "off",
    "semi": "off",
    "object-curly-spacing": "off",
    "quotes": "off"
  },
  "plugins": [
    "react"
  ]
}
