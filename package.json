{
  "name": "redux-form-with-ajv",
  "version": "0.3.0",
  "description": "validate your redux forms with ajv (json-schema)",
  "scripts": {
    "build": "babel src --out-dir ./ --ignore **/*.spec.js",
    "lint": "eslint src",
    "test": "nyc --reporter=html --reporter=text mocha ./src/index.spec.js --compilers js:babel-core/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "examples:simple": "node webpack.server.dev.js ./examples/simple/webpack.config.js",
    "examples:fieldArrays": "node webpack.server.dev.js ./examples/fieldArrays/webpack.config.js"
  },
  "main": "index.js",
  "author": "Jan Salloth aka < yuki ^.^ >",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gitjs/redux-form-with-ajv"
  },
  "keywords": [
    "validation",
    "redux-form",
    "ajv",
    "json-schema"
  ],
  "dependencies": {
    "ajv": "^5.2.4",
    "ajv-errors": "^1.0.0",
    "object-assign": "^4.1.1",
    "object-path": "^0.11.4"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-spies-next": "^0.9.3",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.0",
    "eslint": "^4.9.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.0",
    "eventsource-polyfill": "0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-loader": "^0.5.1",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.7",
    "markdown-loader": "^2.0.1",
    "mocha": "^4.0.1",
    "nyc": "^11.1.0",
    "raw-loader": "0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-form": "^7.1.1",
    "redux-form-website-template": "0.0.41",
    "rimraf": "^2.5.4",
    "webpack": "^3.8.1",
    "webpack-bundle-size-analyzer": "^2.6.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.20.0"
  }
}
